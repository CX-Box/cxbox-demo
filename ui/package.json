{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "cross-env CI=false react-scripts build && npm run rename",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "gen:schema": "node ./node_modules/@cxbox-ui/schema/bin/build-schema",
    "rename": "node -e \"require('fs').rename('build/index.html', 'build/index.ftl', function(err) { if (err) console.log(err); console.log('index.ftl formed') })\""
  },
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@ant-design/icons": "^5.1.4",
    "@ant-design/plots": "^1.2.5",
    "@cxbox-ui/core": "^1.34.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^10.4.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.20.15",
    "@types/react": "^16.14.10",
    "@types/react-dom": "^16.9.13",
    "@types/react-redux": "^7.1.16",
    "antd": "^5.6.0",
    "axios": "^1.5.0",
    "classnames": "^2.3.1",
    "diff": "4.0.2",
    "history": "4.9.0",
    "keycloak-js": "^14.0.0",
    "marked": "0.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.1.2",
    "react-scripts": "^4.0.3",
    "react-window": "1.8.5",
    "redux-observable": "^2.0.0",
    "rxjs": "^7.8.1",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "curly": "error",
      "prettier/prettier": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": ".",
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@types/diff": "4.0.2",
    "@types/history": "4.7.2",
    "@types/marked": "0.7.2",
    "@types/react-window": "^1.8.5",
    "craco-less": "^2.0.0",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "http-proxy-middleware": "^2.0.1",
    "prettier": "^2.3.2",
    "react-app-alias": "^2.2.2",
    "source-map-loader": "0.2.4"
  }
}
