{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "cross-env CI=false craco build --config craco.prod.config.js && npm run rename",
    "start-noKeycloak": "cross-env REACT_APP_NO_SSO=true craco start",
    "build-noKeycloak": "cross-env REACT_APP_NO_SSO=true CI=false craco build --config craco.prod.config.js && npm run rename",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "gen:schema": "node ./node_modules/@cxbox-ui/schema/bin/build-schema",
    "rename": "node -e \"require('fs').rename('build/index.html', 'build/index.ftl', function(err) { if (err) console.log(err); console.log('index.ftl formed') })\""
  },
  "dependencies": {
    "@ant-design/charts": "1.4.2",
    "@cxbox-ui/core": "^1.37.3-alpha.9",
    "@reduxjs/toolkit": "1.9.5",
    "@stomp/stompjs": "7.0.0",
    "antd": "3.26.20",
    "axios": "1.5.0",
    "classnames": "2.3.2",
    "diff": "5.1.0",
    "exceljs": "4.3.0",
    "file-saver": "2.0.5",
    "history": "5.3.0",
    "i18next": "23.4.6",
    "keycloak-js": "14.0.0",
    "mime-types": "2.0.0",
    "parse-data-url": "^6.0.0",
    "pdfjs-dist": "^4.0.379",
    "query-string": "5.1.1",
    "rc-image": "^7.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-file-viewer": "1.2.1",
    "react-i18next": "13.2.1",
    "react-pdf": "^7.7.0",
    "react-redux": "7.2.9",
    "react-scripts": "5.0.1",
    "redux-observable": "2.0.0",
    "rxjs": "7.8.1",
    "uuid": "9.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "curly": "error",
      "prettier/prettier": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": ".",
  "devDependencies": {
    "@craco/craco": "7.1.0",
    "@cxbox-ui/schema": "^0.7.1",
    "@types/diff": "5.0.3",
    "@types/file-saver": "2.0.5",
    "@types/mime-types": "^2.1.4",
    "@types/parse-data-url": "^3.0.1",
    "@types/query-string": "5.1.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/react-redux": "7.1.26",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "craco-less": "3.0.1",
    "cross-env": "7.0.3",
    "eslint": "8.48.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-prettier": "3.4.1",
    "http-proxy-middleware": "2.0.6",
    "prettier": "2.8.8",
    "react-app-alias": "2.2.2",
    "source-map-loader": "4.0.1",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@4.0.2",
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "react-error-overlay": "6.0.9"
  }
}
