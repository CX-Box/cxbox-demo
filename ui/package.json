{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "cross-env CI=false craco build --config craco.prod.config.js && npm run rename",
    "start-noKeycloak": "cross-env REACT_APP_NO_SSO=true craco start",
    "build-noKeycloak": "cross-env REACT_APP_NO_SSO=true CI=false craco build --config craco.prod.config.js && npm run rename",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "gen:schema": "node ./node_modules/@cxbox-ui/schema/bin/build-schema",
    "rename": "node -e \"require('fs').rename('build/index.html', 'build/index.ftl', function(err) { if (err) console.log(err); console.log('index.ftl formed') })\""
  },
  "dependencies": {
    "@ant-design/plots": "1.2.6",
    "@cxbox-ui/core": "2.5.3-beta.5",
    "@reduxjs/toolkit": "1.7.2",
    "@stomp/stompjs": "7.0.0",
    "antd": "3.26.20",
    "axios": "1.7.4",
    "classnames": "2.3.2",
    "diff": "5.1.0",
    "exceljs": "4.3.0",
    "file-saver": "2.0.5",
    "history": "5.3.0",
    "i18next": "23.4.6",
    "keycloak-js": "14.0.0",
    "lodash.debounce": "^4.0.8",
    "mime-types": "2.0.0",
    "parse-data-url": "^6.0.0",
    "query-string": "5.1.1",
    "rc-select": "14.11.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-drag-listview": "2.0.0",
    "react-i18next": "13.2.1",
    "react-redux": "7.2.9",
    "redux-observable": "2.0.0",
    "rxjs": "7.8.1",
    "uuid": "9.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "curly": "error",
      "prettier/prettier": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": ".",
  "devDependencies": {
    "@craco/craco": "7.1.0",
    "@types/diff": "5.0.3",
    "@types/file-saver": "2.0.5",
    "@types/lodash.debounce": "4.0.9",
    "@types/mime-types": "^2.1.4",
    "@types/parse-data-url": "^3.0.1",
    "@types/query-string": "5.1.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/react-redux": "7.1.26",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "craco-less": "3.0.1",
    "cross-env": "7.0.3",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-config-react-app": "7.0.0",
    "eslint-plugin-prettier": "3.4.1",
    "http-proxy-middleware": "2.0.7",
    "prettier": "2.8.8",
    "react-app-alias": "2.2.2",
    "react-scripts": "5.0.0",
    "source-map-loader": "4.0.1",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@4.0.2",
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "react-error-overlay": "6.0.9",
    "node-fetch": "npm:@favware/skip-dependency@1.1.3",
    "react-scripts/@svgr/webpack": "8.1.0",
    "@babel/traverse": "7.25.3",
    "ip": "2.0.1",
    "braces": "3.0.3",
    "fast-loops": "1.1.4",
    "jsdom/ws": "7.5.10",
    "webpack-dev-server/ws": "8.17.1",
    "webpack-dev-middleware": "5.3.4",
    "follow-redirects": "1.15.6",
    "express": "4.19.2",
    "postcss": "8.4.33",
    "@surma/rollup-plugin-off-main-thread/ejs": "3.1.10",
    "fmin/rollup": "npm:@favware/skip-dependency@1.1.3",
    "workbox-build/rollup": "2.79.2",
    "webpack-dev-server/http-proxy-middleware": "2.0.7"
  }
}
