{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "cross-env CI=false craco build --config craco.prod.config.js && npm run rename",
    "start-noKeycloak": "cross-env REACT_APP_NO_SSO=true craco start",
    "build-noKeycloak": "cross-env REACT_APP_NO_SSO=true CI=false craco build --config craco.prod.config.js && npm run rename",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "gen:schema": "node ./node_modules/@cxbox-ui/schema/bin/build-schema",
    "rename": "node -e \"require('fs').rename('build/index.html', 'build/index.ftl', function(err) { if (err) console.log(err); console.log('index.ftl formed') })\""
  },
  "dependencies": {
    "@ant-design/charts": "^1.3.6",
    "@cxbox-ui/core": "^1.37.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^10.4.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.20.15",
    "@types/react": "^16.14.10",
    "@types/react-dom": "^16.9.13",
    "@types/react-redux": "^7.1.16",
    "antd": "^3.26.20",
    "axios": "0.19.0",
    "classnames": "^2.3.1",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "keycloak-js": "^14.0.0",
    "query-string": "5.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.0",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.5.12",
    "typescript": "^3.9.10",
    "uuid": "^9.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "curly": "error",
      "prettier/prettier": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": ".",
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@types/file-saver": "^2.0.5",
    "@types/query-string": "5.1.0",
    "craco-less": "^2.0.0",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "http-proxy-middleware": "^2.0.1",
    "prettier": "^2.3.2",
    "source-map-loader": "0.2.4"
  }
}
